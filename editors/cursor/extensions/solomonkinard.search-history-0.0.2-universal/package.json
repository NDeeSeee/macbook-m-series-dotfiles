	"name": "search-history",
	"displayName": "Search History",
	"description": "Search and replace with history",
	"version": "0.0.2",
	"publisher": "solomonkinard",
	"repository": {
		"type": "git",
		"url": "https://github.com/solomonkinard/public"
	},
	"icon": "icon/128.png",
	"keywords": [
		"search",
		"find",
		"memory",
		"recall",
		"replace",
		"list"
	],
	"engines": {
		"vscode": "^1.77.0"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"views": {
			"explorer": [
				{
					"id": "search-history.view",
					"name": "Search History",
					"type": "webview",
					"when": "editorIsOpen && !editorReadonly"
				}
			]
		}
	},
	"scripts": {
		"compile": "npm run esbuild",
		"watch": "tsc -watch -p ./",
		"vscode:prepublish": "npm run clean && npm run esbuild-base -- --minify && npm run copyJS",
		"esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
		"esbuild": "npm run esbuild-base -- --sourcemap",
		"esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"clean": "rm -rf out/*",
		"copyJS": "mkdir -p out/src/webviews; minify src/webviews/script.js > out/src/webviews/script.js"
	},
	"devDependencies": {
		"@types/glob": "^8.0.0",
